#include <physical_layouts.dtsi>
#include <dt-bindings/zmk/matrix_transform.h>

/ {
    chosen {
        zmk,physical-layout = &physical_layout0;
    };

    default_transform: keymap_transform_0 {
        compatible = "zmk,matrix-transform";
        columns = <10>;
        rows = <4>;
// | SW2  | SW3  | SW4  | SW5  | SW6  |   | SW6  | SW5  | SW4  | SW3  | SW2  |
// | SW8  | SW9  | SW10 | SW11 | SW12 |   | SW12 | SW11 | SW10 | SW9  | SW8  |
// | SW14 | SW15 | SW16 | SW17 | SW18 |   | SW18 | SW17 | SW16 | SW15 | SW14 |
//               | SW19 | SW20 | SW21 |   | SW21 | SW20 | SW19 |
        map = <
RC(0,1) RC(0,2) RC(0,3) RC(0,4) RC(0,5)  RC(0,6) RC(0,7) RC(0,8) RC(0,9) RC(0,10)
RC(1,1) RC(1,2) RC(1,3) RC(1,4) RC(1,5)  RC(1,6) RC(1,7) RC(1,8) RC(1,9) RC(1,10)
RC(2,1) RC(2,2) RC(2,3) RC(2,4) RC(2,5)  RC(2,6) RC(2,7) RC(2,8) RC(2,9) RC(2,10)
                RC(3,3) RC(3,4) RC(3,5)  RC(3,6) RC(3,7) RC(3,8)
        >;
    };

    kscan0: kscan {
        compatible = "zmk,kscan-gpio-matrix";
        wakeup-source;

        diode-direction = "col2row";
        row-gpios
            = <&pro_micro 19 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&pro_micro 18 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&pro_micro 15 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&pro_micro 14 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            ;
    };

    physical_layout0: physical_layout {
        compatible = "zmk,physical-layout";
        display-name = "ztsv cosmos 3x5+3";
        transform = <&default_transform>;
        kscan = <&kscan0>;

        keys  //                     w   h    x    y     rot    rx    ry
            = <&key_physical_attrs 100 100    0  145       0     0     0>
            , <&key_physical_attrs 100 100  108   55       0     0     0>
            , <&key_physical_attrs 100 100  213    0       0     0     0>
            , <&key_physical_attrs 100 100  310   44     100   360    94>
            , <&key_physical_attrs 100 100  405   44     100   455    94>
            , <&key_physical_attrs 100 100 1095   44  (-100)  1145    94>
            , <&key_physical_attrs 100 100 1191   44  (-100)  1241    94>
            , <&key_physical_attrs 100 100 1287    0       0     0     0>
            , <&key_physical_attrs 100 100 1393   55       0     0     0>
            , <&key_physical_attrs 100 100 1500  145       0     0     0>
            , <&key_physical_attrs 100 100    3  257       0     0     0>
            , <&key_physical_attrs 100 100  111  167       0     0     0>
            , <&key_physical_attrs 100 100  218  112       0     0     0>
            , <&key_physical_attrs 100 100  316  156     100   366   206>
            , <&key_physical_attrs 100 100  412  157     100   462   207>
